<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="kr.hi.boot.dao.LikeDAO">
    <insert id="insertLike">
        insert into `Like` (li_state, li_me_id, li_po_num)
        values (1, #{id}, #{postNum})
    </insert>
    <delete id="deleteLike">
        delete from `Like`
        where li_po_num = #{postNum} and li_me_id = #{id}
    </delete>
</mapper>